
<div class="row">
  <div class="col-xs-2 noPad">
    <div class="row voteDisplay">
      <div class="col-sm-5 noPad">
        <div class="voteNumbers">
          <%= question.escape('sum_votes') || 0 %> 
        </div>
        <div class="voteText">
          votes
        </div>
      </div>
      <div class="col-sm-7 noPad">
        <div class="voteNumbers">
          <%= question.escape('num_answers') || 0 %>
        </div>
        <div class="voteText">
          answers
        </div>
      </div>
    </div>
  </div>
    
  <div class="col-xs-10">  
  <h4><a href="/#/questions/<%= question.get('id') %>" class="menuLink"><%= question.escape('title') %></a></h4>

  <p>asked by <a href="/#/users/<%= question.get('user_id') %>"><%= question.escape('username') %></a> <%= timeSince(question.get('created_at')) %> ago</p>
  </div>

</div>