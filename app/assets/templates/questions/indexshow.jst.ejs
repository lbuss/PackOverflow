
<div class="row">
<div class="col-xs-2 noPad">
  <div class="row voteNumbers voteDisplay">
    <div class="col-xs-6 noPad">
      <%= question.escape('sumVotes') || 0 %>
    </div>
    <div class="col-xs-6 noPad">
      <%= question.escape('sumVotes') || 0 %>
    </div>
  
  </div>
  <div class="row voteText voteDisplay">
    <div class="col-xs-6 noPad">
      votes
    </div>
    <div class="col-xs-6 noPad">
      answers
    </div>
  </div>
</div>
  
<div class="col-xs-10">  
<h4><a href="/#/questions/<%= question.get('id') %>" class="menuLink"><%= question.escape('title') %></a></h4>

<p>asked by <a href="/#/users/<%= question.get('user_id') %>"><%= question.escape('username') %></a> <%= timeSince(question.get('created_at')) %> ago</p>
</div>

</div>