
<div class="page-header col-xs-12"><h4><%= question.escape('title') %></h4></div>

<div class="row">
<div id="questionVoteBox" class="voteBox col-xs-1"></div>
<div class="col-xs-11">
  <div class="row">
    <div class="col-xs-12">
      <div id="answerBody"><%= question.get('body') %></div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <p>asked by <a href="/#/users/<%= question.get('user_id') %>"><%= question.username %></a> at <%= question.escape('created_at') %></p>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <ul id="questionCommentList" class="commentList"></ul>
      <div id="questionCommentForm" class="form"></div>
    </div>
  </div>
 
</div>

<ul class="answerList"></ul>
  
<div id="answerForm" class="form"></div>